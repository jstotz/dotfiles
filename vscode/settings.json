{
  "workbench.startupEditor": "newUntitledFile",
  "keyboard.dispatch": "keyCode",
  "[ruby]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[Gemfile]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "files.exclude": {
    ".vscode": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    ".byebug_history": true,
    "tmp": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.autoClosingBrackets": "beforeWhitespace",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "eslint.packageManager": "yarn",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.organizeImports",
      "source.formatDocument"
    ]
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.organizeImports",
      "source.formatDocument"
    ]
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[graphql]": {
    "editor.formatOnSave": true
  },
  "terminal.integrated.rendererType": "dom",
  "vs-kubernetes": {
    "vs-kubernetes.minikube-path": "/home/jstotz/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.useLanguageServer": true,
  "files.associations": {
    "**/i3/config": "i3",
    "*.mal": "lisp"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03'",
  "workbench.colorTheme": "Tomorrow Night",
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.containers.groupBy": "None",
  "docker.containers.label": "ContainerName",
  "docker.containers.description": ["Status"],
  "gitlens.hovers.currentLine.over": "line",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[makefile]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": false,
    "editor.detectIndentation": false
  },
  "java.home": "/Library/Java/JavaVirtualMachines/openjdk-13.0.2.jdk/Contents/Home",
  "java.help.firstView": "gettingStarted",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "errorLens.addAnnotationTextPrefixes": true,
  "errorLens.addNumberOfDiagnostics": true,
  "errorLens.onSave": true,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "editor.minimap.enabled": false,
  "tsimporter.noStatusBar": true,
  "editor.matchBrackets": false,
  "window.zoomLevel": 2
}
