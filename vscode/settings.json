{
  "workbench.startupEditor": "newUntitledFile",
  "keyboard.dispatch": "keyCode",
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "Shopify.ruby-lsp",
  },
  "[go]": {
    "files.trimTrailingWhitespace": false
  },
  "[plaintext]": {
    "files.trimTrailingWhitespace": false
  },
  "[Gemfile]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "files.exclude": {
    ".byebug_history": true,
    ".vscode": true,
    "**/.classpath": true,
    "**/.DS_Store": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.stfolder": true,
    "**/.svn": true,
    "tmp": true
  },
  "editor.autoClosingBrackets": "never",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.organizeImports",
      "source.formatDocument"
    ]
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.organizeImports",
      "source.formatDocument"
    ]
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[graphql]": {
    "editor.formatOnSave": true
  },
  "editor.suggestSelection": "first",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.useLanguageServer": true,
  "files.associations": {
    "**/i3/config": "i3",
    "*.mal": "lisp"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03'",
  "workbench.colorTheme": "Catppuccin Mocha",
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.containers.groupBy": "None",
  "docker.containers.label": "ContainerName",
  "docker.containers.description": ["Status"],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[makefile]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": false,
    "editor.detectIndentation": false
  },
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "editor.minimap.enabled": false,
  "editor.matchBrackets": "never",
  "git.autofetch": true,
  "git.confirmSync": false,
  "json.schemas": [
    {
      "fileMatch": ["/.goreleaser.yaml"],
      "url": "https://goreleaser.com/static/schema.json"
    }
  ],
  "editor.inlineSuggest.enabled": true,
  "scm.defaultViewMode": "tree",
  "terminal.integrated.scrollback": 100000,
  "window.commandCenter": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "git.showCommitInput": false,
  "window.zoomLevel": 2,
  "[python]": {
    "editor.formatOnType": true
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "/bin/zsh",
      "args": [
        "-l",
        "-i"
      ]
    }
  },
  "workbench.activityBar.location": "top",
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "rubyLsp.formatter": "syntax_tree",
  "github.copilot.chat.languageContext.fix.typescript.enabled": true,
  "github.copilot.chat.languageContext.inline.typescript.enabled": true,
  "github.copilot.chat.languageContext.typescript.enabled": true
}
