{
  "workbench.startupEditor": "newUntitledFile",
  "keyboard.dispatch": "keyCode",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-c>": false,
    "<C-f>": false,
    "<C-n>": false,
    "<C-a>": false,
    "<C-w>": false
  },
  "vim.useSystemClipboard": true,
  "[ruby]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "files.exclude": {
    ".vscode": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    ".byebug_history": true,
    "tmp": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.autoClosingBrackets": "beforeWhitespace",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "eslint.packageManager": "yarn",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[graphql]": {
    "editor.formatOnSave": true
  },
  "terminal.integrated.rendererType": "dom",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "vs-kubernetes": {
    "vs-kubernetes.minikube-path": "/home/jstotz/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.useLanguageServer": true,
  "files.associations": {
    "**/i3/config": "i3"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03'",
  "workbench.colorTheme": "Tomorrow Night",
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.containers.groupBy": "None",
  "docker.containers.label": "ContainerName",
  "docker.containers.description": [
    "Status"
  ]
}
