{
  "workbench.startupEditor": "newUntitledFile",
  "keyboard.dispatch": "keyCode",
  "ruby.lint": {
    "rubocop": true
  },
  "[ruby]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[go]": {
    "files.trimTrailingWhitespace": false
  },
  "[plaintext]": {
    "files.trimTrailingWhitespace": false
  },
  "[Gemfile]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "files.exclude": {
    ".byebug_history": true,
    ".vscode": true,
    "**/.classpath": true,
    "**/.DS_Store": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.stfolder": true,
    "**/.svn": true,
    "tmp": true
  },
  "editor.autoClosingBrackets": "never",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "eslint.packageManager": "yarn",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.organizeImports",
      "source.formatDocument"
    ]
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.organizeImports",
      "source.formatDocument"
    ]
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[graphql]": {
    "editor.formatOnSave": true
  },
  "terminal.integrated.rendererType": "dom",
  "vs-kubernetes": {
    "vs-kubernetes.minikube-path": "/home/jstotz/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.useLanguageServer": true,
  "files.associations": {
    "**/i3/config": "i3",
    "*.mal": "lisp"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03'",
  "workbench.colorTheme": "Tomorrow Night",
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.containers.groupBy": "None",
  "docker.containers.label": "ContainerName",
  "docker.containers.description": ["Status"],
  "gitlens.hovers.currentLine.over": "line",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[makefile]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": false,
    "editor.detectIndentation": false
  },
  "java.home": "/Library/Java/JavaVirtualMachines/openjdk-13.0.2.jdk/Contents/Home",
  "java.help.firstView": "gettingStarted",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "errorLens.addAnnotationTextPrefixes": true,
  "errorLens.addNumberOfDiagnostics": true,
  "errorLens.onSave": true,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "editor.minimap.enabled": false,
  "tsimporter.noStatusBar": true,
  "editor.matchBrackets": "never",
  "git.autofetch": true,
  "go.toolsManagement.autoUpdate": true,
  "vscode-neovim.mouseSelectionStartVisualMode": true,
  "git.confirmSync": false,
  "json.schemas": [
    {
      "fileMatch": ["/.goreleaser.yaml"],
      "url": "https://goreleaser.com/static/schema.json"
    }
  ],
  "projectManager.git.baseFolders": ["~/Projects"],
  "editor.inlineSuggest.enabled": true,
  "advancedNewFile.exclude": {
    "node_modules": true,
    "node_modules_electron": true,
    "dev": true,
    "dist": true
  },
  "advancedNewFile.showInformationMessages": true,
  "scm.defaultViewMode": "tree",
  "githubPullRequests.createOnPublishBranch": "never",
  "terminal.integrated.scrollback": 100000,
  "window.commandCenter": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "git.showCommitInput": false,
  "githubPullRequests.pullBranch": "never",
  "workbench.editor.untitled.hint": "hidden",
  "projectManager.sortList": "Recent",
  "window.zoomLevel": 2
}
